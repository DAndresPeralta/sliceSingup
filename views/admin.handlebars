<main id="seccion-principal">
  <section id="firstWrapI" class="conteiner">
    <div>
      <h1 class="title">Registro</h1>
      <form id="singupForm">
        <label class="lbl" for="name">
          <i class="fa-solid fa-user-tie"></i>
          <input
            type="text"
            name="nombre"
            id="nameS"
            placeholder="Ingrese su nombre"
          />
        </label>
        <label for="email">
          <i class="fa-solid fa-at"></i>
          <input
            type="email"
            name="email"
            id="emailS"
            placeholder="Ingrese su correo"
          />
        </label>
        <label for="password"><i class="fa-solid fa-lock"></i><input
            type="password"
            name="password"
            id="passwordS"
            class="reset"
            placeholder="Ingrese una contraseña"
          /></label>
        <button type="submit" id="btnS">Registrarme</button>
      </form>
    </div>
  </section>
